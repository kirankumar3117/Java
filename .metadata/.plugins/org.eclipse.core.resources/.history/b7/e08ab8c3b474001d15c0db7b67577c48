package com.classassignment;

public class Instructor extends Person{
	int instrcutorId;
	int salary;
	@Override
	public String toString() {
		return "Instructor [instrcutorId=" + instrcutorId + ", salary=" + salary + ", city=" + city + ", state=" + state
				+ ", pincode=" + pincode + ", Address()=" + Address() + "]";
	}
}
