package com.classassignment;

public class Adderess {
	String city="chennai";
	String state="TN";
	String 	pincode="521180";
	
	
	
	@Override
	public String toString() {
		return "Adderess [city=" + city + ", state=" + state + ", pincode=" + pincode + "]";
	}



	public static void main(String[] args) {
		Adderess a1=new Adderess();
		
		System.out.println(a1);
	}
}
